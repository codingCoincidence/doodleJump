<launch4jConfig>
  <headerType>gui</headerType>
  <jar>lib/doodlejump.jar</jar>
  <outfile>../../build/DoodleJump.exe</outfile>
  <errTitle>doodlejump</errTitle>
  <chdir>.</chdir>
